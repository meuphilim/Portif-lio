{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@vercel/next" }],
  "routes": [
    { "src": "/api/generate", "dest": "/api/generate.js" },
    { "src": "/api/assets/(.*)", "dest": "/api/assets/$1" },
    { "src": "/api/projects/(.*)", "dest": "/api/projects/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "env": {
    "GITHUB_USERNAME": "meuphilim",
    "NEXT_PUBLIC_GITHUB_USERNAME": "meuphilim",
    "GITHUB_ACTIONS": "true"
  }
}
